    </br>             
    <h3 class="text-center font-weight-bold">List User</h3>
    </br>
    <%= paginate @users %> 
    <table class="table">
    
      <thead >
        <tr>
          <th>ID</th>
          <th>Email</th>
          <th>Full Name</th>  
          <th>Phone Number</th>
          <th>Date Create</th>                    
          <th colspan="3">Action</th>                             
          <%# <th class="col-1"></th> 
          <th class="col-2"></th>  %>
        </tr>              
      </thead>
      <tbody>
          <% @users.each do |user| %>
            <tr>
              <td ><%= user.id %></td>  
              <td ><%= user.email %></td>
              <td ><%= user.full_name %></td>
              <td ><%= user.phone_number %></td>
              <td ><%= user.created_at %></td>     
              <td ><%= link_to 'Show', '' %></td>
              <td ><%= link_to 'Edit', '' %></td>
              <td ><%= link_to 'Destroy', '', method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>                    
          <% end %>                    
      </tbody>              
    </table>
      